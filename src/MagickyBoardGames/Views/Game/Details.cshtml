@model MagickyBoardGames.ViewModels.GameViewViewModel

<div style="float: right">
    <a asp-action="Edit" asp-route-id="@Model.Game.Id" data-toggle="tooltip" title="Edit" class="fa fa-pencil fa-2x"></a>
    <a asp-action="Delete" asp-route-id="@Model.Game.Id" data-toggle="tooltip" title="Delete" class="fa fa-trash-o fa-2x" style="color: red"></a>
</div>
@await Html.PartialAsync("_Info", Model.Game)
@await Html.PartialAsync("_Categories", Model.Categories)
@await Html.PartialAsync("_Owners", Model.Owners)
@await Html.PartialAsync("_PlayerRatings", Model.PlayerRatings)
<ul class="pager">
    <li><a asp-action="Index">Back to List</a></li>
</ul>